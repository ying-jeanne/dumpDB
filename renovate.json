{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchBaseBranches": ["main"],
      "enabled": true
    },
    {
      "matchBaseBranches": ["^release-.*"],
      "enabled": true      
    }
  ]
  "vulnerabilityAlerts": {
    "enabled": true
  }
}
